{
  "name": "@uora/swc",
  "version": "0.0.0-alpha.0",
  "description": "uora",
  "keywords": [
    "uora"
  ],
  "homepage": "https://uora.io",
  "bugs": {
    "url": "https://github.com/uora/uora/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/uora/uora.git",
    "directory": "packages/utils"
  },
  "license": "MIT",
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "dist",
    "index.js",
    "index.d.ts"
  ],
  "scripts": {
    "build": "napi build --cargo-cwd ./crates/napi --platform dist --release",
    "build:debug": "napi build --platform dist",
    "dev": "pnpm build",
    "prepublishOnly": "napi prepublish -t npm"
  },
  "devDependencies": {
    "@napi-rs/cli": "^2.16.2",
    "@napi-rs/triples": "1.2.0"
  },
  "engines": {
    "node": "^14.19.0 || >=16.10.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "authors": [
    "Zack <lorenx@163.com> (https://github.com/loyep)"
  ],
  "napi": {
    "name": "uora-swc",
    "triples": {
      "defaults": true,
      "additional": [
        "x86_64-unknown-linux-musl",
        "x86_64-unknown-freebsd",
        "aarch64-unknown-linux-gnu"
      ]
    }
  }
}
