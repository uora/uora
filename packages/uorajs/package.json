{
  "name": "uorajs",
  "version": "0.0.0-alpha.0",
  "description": "uora",
  "keywords": [
    "uora"
  ],
  "homepage": "https://uora.io",
  "bugs": {
    "url": "https://github.com/uora/uora/issues"
  },
