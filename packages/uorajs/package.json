  "scripts": {
    "build": "tsup",
    "dev": "pnpm build --watch",
    "prepack": "pnpm build"
  },
  "dependencies": {
    "@fastify/deepmerge": "^1.3.0",
    "@rollup/plugin-virtual": "^3.0.1",
