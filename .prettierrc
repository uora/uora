{
  "printWidth": 120,
  "tabWidth": 2,
  "semi": false,
  "bracketSameLine": true,
  "singleQuote": true,
  "trailingComma": "all",
  "overrides": [
    {
      "files": ".prettierrc",
      "options": {
        "parser": "json"
      }
    }
  ],
  "plugins": ["prettier-plugin-packagejson", "prettier-plugin-organize-imports"]
}
